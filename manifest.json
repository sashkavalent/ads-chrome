{
  "manifest_version": 2,

  "name": "Ads",
  "description": "This extension loads ads data.",
  "version": "1.0",

  "permissions": [
    "http://*/"
  ],
  "externally_connectable": {
    "matches": ["http://localhost:3000/"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
